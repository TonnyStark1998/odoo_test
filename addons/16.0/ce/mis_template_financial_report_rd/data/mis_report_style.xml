<?xml version="1.0" encoding="UTF-8"?>
<odoo noupdate="1">
    <record id="mis_style_total" model="mis.report.style">
        <field name="name">Total</field>
        <field name="color_inherit" eval="False" />
        <field name="color">#FFFFFF</field>
        <field name="background_color_inherit" eval="False" />
        <field name="background_color">#7A6571</field>
        <field name="font_weight_inherit" eval="False" />
        <field name="font_weight">bold</field>
        <field name="font_size_inherit" eval="False" />
        <field name="font_size">large</field>
    </record>
    <record id="mis_style_default" model="mis.report.style">
        <field name="name">Default</field>
        <field name="dp_inherit">false</field>
        <field name="dp">2</field>
        <field name="hide_empty_inherit">false</field>
        <field name="hide_empty">true</field>
    </record>
    <record id="mis_style_header" model="mis.report.style">
        <field name="name">Header</field>
        <field name="font_weight_inherit">false</field>
        <field name="font_weight">bold</field>
    </record>
    <record id="mis_style_header_l1" model="mis.report.style">
        <field name="name">Header L1</field>
        <field name="font_weight_inherit">false</field>
        <field name="font_weight">bold</field>
        <field name="indent_level_inherit">false</field>
        <field name="indent_level">1</field>
    </record>
    <record id="mis_style_details_l2" model="mis.report.style">
        <field name="name">Details L2</field>
        <field name="indent_level_inherit">false</field>
        <field name="indent_level">2</field>
    </record>
</odoo>
